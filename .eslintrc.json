{
    "env": {
        "browser": true,
        "es2020": true
    },
    "plugins": ["lodash", "promise"],
    "extends": [
        "airbnb-base",
        "plugin:lodash/recommended",
        "plugin:promise/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "lodash/prefer-lodash-method": [2, {"ignoreMethods": ["find"]}],
        "no-console": 0
    }
}
